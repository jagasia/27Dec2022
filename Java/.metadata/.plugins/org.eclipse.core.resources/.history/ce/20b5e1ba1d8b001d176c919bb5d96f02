
public class Main2 {

	public static void main(String[] args) {
		Student s=new Student();
		JavaStudent js=new JavaStudent();
		js=(JavaStudent) s;		//narrowing needs explicit casting
		
		s=js;		//implicit casting	widening
		Object o=new Object();
		
		o=js;
		o=s;
		
		byte b=10;
		b+=1;
		
		
	}

}
