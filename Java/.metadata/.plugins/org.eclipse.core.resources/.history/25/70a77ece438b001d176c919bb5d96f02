import java.util.Scanner;

public class MainPalindrome {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String input=sc.next();
		boolean flag=true;
		
		int len=input.length();
		
		for(int i=0;i<len;i++)
		{
			if(input.charAt(i)!=input.charAt(len-i-1))
			{
				flag=false;
				break;
			}
		}
	}

}
